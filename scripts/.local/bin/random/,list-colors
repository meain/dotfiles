#!/bin/sh

printf "[0m16 colors\n"
for code in {30..37}; do
	echo -n "[0;${code}m$(printf %03d $code) "
done
for code in {90..97}; do
	echo -n "[0;${code}m$(printf %03d $code) "
done

printf "[0m\n\n256 colors\n"

for code in {0..255}; do
	echo -n "[38;05;${code}m$(printf %03d $code) "
	[ $((${code} % 16)) -eq 15 ] && echo
done
