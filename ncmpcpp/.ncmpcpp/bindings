def_key "+"
    show_clock
def_key "="
    volume_up

def_key "j"
    scroll_down
def_key "k"
    scroll_up

def_key "ctrl-u"
    page_up

def_key "ctrl-d"
    page_down

def_key "u"
    page_up

def_key "d"
    page_down

def_key "h"
    previous_column
def_key "l"
    next_column

def_key "."
    show_lyrics

def_key "n"
    next_found_item
def_key "N"
    previous_found_item

def_key "r"
 toggle_repeat

def_key "z"
 toggle_random


def_key "c"
 stop
def_key "p"
 pause

def_key ">"
 next
def_key "<"
 previous

